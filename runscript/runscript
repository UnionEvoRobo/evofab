if [ "$if [ "$PBS_ENVIRONMENT" != "PBS_INTERACTIVE" ] ; then
    cd $PBS_O_WORKDIR
fi

typeset integer NPROCS
NPROCS=`wc -l $PBS_NODEFILE | awk '{print $1}'`
uniq $PBS_NODEFILE | sed -e 's/^/host /g' -e 's/$/-eth0/g' >/tmp/nodes
NODES="/tmp/nodes"

python
